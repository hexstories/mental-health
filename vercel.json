{
    "builds": [{
        "src":"core/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize":"15mb", "runtime": "python3.9"}
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "static_root"
        }
      }
],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}